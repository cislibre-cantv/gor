  `id_cnx_ordenes` int(11) NOT NULL AUTO_INCREMENT,
  `co_orden` int(11) NOT NULL,
  `nro_orden_serv` varchar(30) COLLATE latin1_general_cs NOT NULL,
  `orden_confirmacion` varchar(2) COLLATE latin1_general_cs DEFAULT NULL,
  `orden_status` varchar(15) COLLATE latin1_general_cs NOT NULL,
  `orden_telefono` varchar(15) COLLATE latin1_general_cs DEFAULT NULL,
  `orden_central` varchar(6) COLLATE latin1_general_cs DEFAULT NULL,
  `orden_observacion` varchar(250) COLLATE latin1_general_cs DEFAULT NULL,
  `orden_accion` varchar(3) COLLATE latin1_general_cs DEFAULT NULL,
  `usuario` varchar(50) COLLATE latin1_general_cs DEFAULT NULL,
  `fecha` varchar(25) COLLATE latin1_general_cs DEFAULT NULL,
  `orden_lni` varchar(2) COLLATE latin1_general_cs DEFAULT NULL,
