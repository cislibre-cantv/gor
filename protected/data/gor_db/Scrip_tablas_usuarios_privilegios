-- --------------------------------------------------------

--
-- Table structure for table `cnx_asig_localidades_grupos`
--

CREATE TABLE IF NOT EXISTS `cnx_asig_localidades_grupos` (
  `id_localidad_grupo` int(11) NOT NULL AUTO_INCREMENT,
  `co_grupo` int(11) NOT NULL,
  `co_localidad` int(11) NOT NULL,
  `fe_crea` datetime DEFAULT NULL,
  `fe_modf` datetime DEFAULT NULL,
  `usr_crea` varchar(10) DEFAULT NULL,
  `usr_modf` varchar(10) DEFAULT NULL,
  `in_stat` varchar(1) DEFAULT 'A',
  `tx_desc` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id_localidad_grupo`),
  UNIQUE KEY `id_localidad_grupo_unique` (`id_localidad_grupo`),
  KEY `fk_asig_co_grupo` (`co_grupo`),
  KEY `fk_asig_co_localidad` (`co_localidad`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `cnx_asig_loc_orden`
--

CREATE TABLE IF NOT EXISTS `cnx_asig_loc_orden` (
  `id_asig_loc_orden` int(11) NOT NULL,
  `co_asig_loc_orden` int(11) NOT NULL,
  `co_localidad` int(11) NOT NULL,
  `co_orden` int(11) NOT NULL,
  `in_stat` varchar(2) COLLATE latin1_general_cs DEFAULT NULL,
  `tx_obs` varchar(500) COLLATE latin1_general_cs DEFAULT NULL,
  `co_tipo_servicio` int(11) DEFAULT NULL,
  `co_status_serv` int(11) DEFAULT NULL,
  `fe_crea` datetime DEFAULT NULL,
  `fe_modf` datetime DEFAULT NULL,
  `usr_crea` varchar(10) COLLATE latin1_general_cs DEFAULT NULL,
  `usr_modf` varchar(10) COLLATE latin1_general_cs DEFAULT NULL,
  PRIMARY KEY (`co_asig_loc_orden`),
  KEY `co_tipo_servicio` (`co_tipo_servicio`),
  KEY `fk_orden_asig_ord` (`co_orden`),
  KEY `fk_orden_asig_loc` (`co_localidad`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_general_cs;

-- --------------------------------------------------------

--
-- Table structure for table `cnx_entes_localidades`
--

CREATE TABLE IF NOT EXISTS `cnx_entes_localidades` (
  `id_sector_proyecto` int(11) NOT NULL AUTO_INCREMENT,
  `co_sector_proyecto` int(11) NOT NULL,
  `nb_entidad` varchar(50) COLLATE latin1_general_cs NOT NULL,
  `email` varchar(45) COLLATE latin1_general_cs DEFAULT NULL,
  `cnx_sectores_proyectos` varchar(20) COLLATE latin1_general_cs DEFAULT NULL,
  `fe_crea` datetime DEFAULT NULL,
  `fe_modf` varchar(10) COLLATE latin1_general_cs DEFAULT NULL,
  `usr_crea` varchar(10) COLLATE latin1_general_cs DEFAULT NULL,
  `usr_modf` varchar(10) COLLATE latin1_general_cs DEFAULT NULL,
  `in_stat` varchar(1) COLLATE latin1_general_cs DEFAULT 'A',
  `tx_desc` varchar(45) COLLATE latin1_general_cs DEFAULT NULL,
  PRIMARY KEY (`co_sector_proyecto`),
  UNIQUE KEY `id_sector_proyecto_unique` (`id_sector_proyecto`),
  UNIQUE KEY `co_sector_proyecto_unique` (`co_sector_proyecto`)
