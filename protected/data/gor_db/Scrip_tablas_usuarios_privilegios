  `id_grupo` int(11) NOT NULL AUTO_INCREMENT,
  `co_grupo` int(11) NOT NULL,
  `nb_grupo` varchar(100) NOT NULL,
  `fe_crea` datetime DEFAULT NULL,
  `fe_modf` datetime DEFAULT NULL,
  `usr_crea` varchar(10) DEFAULT NULL,
  `usr_modf` varchar(10) DEFAULT NULL,
  `in_stat` varchar(1) DEFAULT 'A',
  `tx_desc` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`co_grupo`),
  UNIQUE KEY `id_grupo_unique` (`id_grupo`),
  UNIQUE KEY `co_grupo_unique` (`co_grupo`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 AUTO_INCREMENT=1 ;

-- --------------------------------------------------------

--
-- Table structure for table `cnx_localidades`
--

CREATE TABLE IF NOT EXISTS `cnx_localidades` (
  `id_localidad` int(11) NOT NULL AUTO_INCREMENT COMMENT '    ',
  `co_localidad` int(11) NOT NULL,
  `co_tipo_localidad` int(11) NOT NULL,
  `tecnologia_inicial` int(11) DEFAULT NULL,
  `co_ente` int(11) NOT NULL,
  `tecnologia_actual` int(11) DEFAULT NULL,
  `co_region` int(11) NOT NULL,
  `co_edo` int(11) DEFAULT NULL,
  `co_municipio` int(11) NOT NULL,
  `co_parroquia` int(11) NOT NULL,
  `nb_localidad` varchar(100) NOT NULL,
  `direccion` varchar(200) DEFAULT NULL,
  `codigo_localidad` varchar(45) NOT NULL,
  `ciudad` varchar(45) DEFAULT NULL,
  `urbanizacion` varchar(45) DEFAULT NULL,
  `avcalle` varchar(45) DEFAULT NULL,
  `numero` int(11) DEFAULT NULL,
  PRIMARY KEY (`co_localidad`),
  UNIQUE KEY `id_localidad_unique` (`id_localidad`),
  UNIQUE KEY `co_localidad_unique` (`co_localidad`),
  UNIQUE KEY `codigo_localidad_unique` (`codigo_localidad`),
  KEY `fk_cnx_loc_co_region` (`co_region`),
  KEY `fk_cnx_loc_co_edo` (`co_edo`),
  KEY `fk_cnx_loc_co_munc` (`co_municipio`),
  KEY `fk_cnx_loc_co_prrq` (`co_parroquia`),
  KEY `fk_cnx_loc_co_tipo_loc` (`co_tipo_localidad`),
  KEY `fk_cnx_loc_co_ente_idx` (`co_ente`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COMMENT='almacena informacion de todas las localidades u centros pertenecientes al proyecto conectividad y wifi' AUTO_INCREMENT=1 ;

